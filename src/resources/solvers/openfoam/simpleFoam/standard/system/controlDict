FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application simpleFoam;

startFrom         <<<ComboBox: start from: (specified iteration, startTime), (last iteration, lastTime): startTime>>>;
startTime         <<<IntLineEdit: start iteration: : 0>>>;
stopAt            endTime;
endTime           <<<IntLineEdit: last iteration: : 1000>>>;
deltaT            1;
writeControl      timeStep;
writeInterval     <<<IntLineEdit: write interval: : 100>>>;
purgeWrite        0;
writeFormat       binary;
writePrecision    6;
writeCompression  uncompressed;
timeFormat        general;
timePrecision     6;
runTimeModifiable no;

